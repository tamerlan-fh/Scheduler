<Window x:Class="Scheduler.TrayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Scheduler"
        xmlns:tb="clr-namespace:Scheduler.TaskbarNotification"
        mc:Ignorable="d"
        Title="Планировщик" Height="350" Width="400" Icon="Resurses/usatu.ico" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" >
    <Grid>
        <tb:TaskbarIcon IconSource="Resurses/usatu.ico" ToolTipText="{Binding Timestamp}">
           
        </tb:TaskbarIcon>
    </Grid>
</Window>
